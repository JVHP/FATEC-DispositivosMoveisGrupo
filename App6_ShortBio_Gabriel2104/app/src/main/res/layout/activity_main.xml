<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <TextView
            android:text="ShortBio Gabriel2104"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/titulo"
            android:textSize="30dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="80dp"/>
    <Button
            android:text="Sobre"
            android:layout_width="180dp"
            android:layout_height="65dp"
            android:id="@+id/sobre"
            android:layout_marginTop="80dp"
            app:layout_constraintTop_toBottomOf="@+id/titulo"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            tools:layout_editor_absoluteY="195dp"
            tools:layout_editor_absoluteX="150dp"/>
    <Button
            android:text="Formação"
            android:layout_width="180dp"
            android:layout_height="65dp"
            android:id="@+id/formacao"
            android:layout_marginTop="60dp"
            app:layout_constraintTop_toBottomOf="@+id/sobre"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            tools:layout_editor_absoluteY="323dp"
            tools:layout_editor_absoluteX="150dp"/>
    <Button
            android:text="Experiência"
            android:layout_width="180dp"
            android:layout_height="65dp"
            android:id="@+id/experiencia"
            android:layout_marginTop="60dp"
            app:layout_constraintTop_toBottomOf="@+id/formacao"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            tools:layout_editor_absoluteY="457dp"
            tools:layout_editor_absoluteX="146dp"/>
    <Button
            android:text="Objetivo"
            android:layout_width="180dp"
            android:layout_height="65dp"
            android:id="@+id/objetivo"
            android:layout_marginTop="60dp"
            app:layout_constraintTop_toBottomOf="@+id/experiencia"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            android:layout_marginBottom="100dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>