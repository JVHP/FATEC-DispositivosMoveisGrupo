<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".objetivo">

    <TextView
            android:text="Objetivos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/titulo"
            android:textSize="30dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="80dp"
            app:layout_constraintHorizontal_bias="0.498"/>
    <ImageView
            android:layout_width="246dp"
            android:layout_height="279dp"
            app:srcCompat="@drawable/bd"
            android:id="@+id/imageView2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/titulo"
            app:layout_constraintHorizontal_bias="0.496"/>
    <TextView
            android:text="Objetivo de carreira é trabalhar com Banco de Dados"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView8"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView2"
            tools:layout_editor_absoluteY="347dp"
            tools:layout_editor_absoluteX="97dp"/>
    <TextView
            android:text="'Em vez de viver mais um dia sem agarrar um sonho que está ao meu alcance, eu prefiro viver meu sonho e agarrar a morte!'"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView9"
            android:layout_marginTop="20dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView8"
            tools:layout_editor_absoluteY="396dp"
            tools:layout_editor_absoluteX="91dp"/>
    <Button
            android:text="Voltar"
            android:layout_width="180dp"
            android:layout_height="65dp"
            android:id="@+id/voltar"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/textView9"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            android:layout_marginBottom="100dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>